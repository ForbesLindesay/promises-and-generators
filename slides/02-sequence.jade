  h2 Sequence 1
  :javascript
    function count(n){
      var res = []
      for (var x = 0; x < n; x++) {
        res.push(x)
      }
      return res
    }
    var res = count(5)
    for (var i = 0; i < res.length; i++) {
      console.log(res[i])
    }

